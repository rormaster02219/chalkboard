<div>
    <%= form_for :result do |f| %>
      <%= f.label :question_id %>
      <%= f.text_field :question_id %>

      <%= f.label :student_id %>
      <%= f.text_field :student_id, :value => current_student.id %>

      <%= f.label :correct %>
      <%= f.text_field :correct %>

      <%= f.label :student_answer %>
      <%= f.text_field :student_answer %>

      <%= f.submit "Create my result" %>
    <% end %>
</div>
